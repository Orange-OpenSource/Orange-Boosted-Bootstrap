<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>boosted.sass</id>
    <!-- pulled from package.json -->
    <version>5</version>
    <title>Boosted Sass</title>
    <authors>The Boosted Authors, Orange SA</authors>
    <owners>Orange SA</owners>
    <description>Orange Boosted with Bootstrap is a Bootstrap based, Orange branded accessible and ergonomic components library.</description>
    <releaseNotes>https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap/releases</releaseNotes>
    <summary>Boosted framework in Sass. Includes fonts and JavaScript</summary>
    <language>en-us</language>
    <projectUrl>https://boosted.orange.com/</projectUrl>
    <repository type="git" url="https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap.git" branch="main" />
    <icon>boosted.png</icon>
    <license type="expression">MIT</license>
    <copyright>Copyright 2015-2025</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>css sass mobile-first responsive front-end framework web bootstrap Orange</tags>
    <contentFiles>
      <files include="**/*" buildAction="Content" />
    </contentFiles>
  </metadata>
  <files>
    <file src="nuget\boosted.png" target="" />

    <file src="scss\**\*.scss" target="content\Content\boosted" />
    <file src="dist\js\boosted*.js" target="content\Scripts" />
    <file src="dist\js\boosted*.js.map" target="content\Scripts" />

    <file src="scss\**\*.scss" target="contentFiles\Content\any\any\wwwroot\scss" />
    <file src="dist\js\boosted*.js" target="contentFiles\any\any\wwwroot\js" />
    <file src="dist\js\boosted*.js.map" target="contentFiles\any\any\wwwroot\js" />
  </files>
