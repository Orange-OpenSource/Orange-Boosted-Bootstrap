<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>boosted.sass</id>
    <version>4.3.1</version>
    <title>Boosted Sass</title>
    <authors>The Boosted Authors, Orange SA</authors>
    <owners>Orange SA</owners>
    <description>Orange Boosted with Bootstrap is a Bootstrap based, Orange branded accessible and ergonomic components library.</description>
    <releaseNotes>https://github.com/Orange-OpenSource/Orange-Boosted-Bootstrap/releases</releaseNotes>
    <summary>Boosted framework in Sass. Includes fonts and JavaScript</summary>
    <language>en-us</language>
    <projectUrl>https://boosted.orange.com/</projectUrl>
    <iconUrl>https://boosted.orange.com/docs/4.3/assets/img/favicons/apple-touch-icon.png</iconUrl>
    <license type="file">LICENSE.txt</license>
    <copyright>Copyright 2017-2020</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="popper.js" version="[1.16.0,2)" />
    </dependencies>
    <tags>css sass mobile-first responsive front-end framework web bootstrap Orange</tags>
  </metadata>
  <files>
    <file src="LICENSE.txt" target="" />

    <file src="scss\**\*.scss" target="content\Content\bootstrap" />
    <file src="dist\js\boosted*.js" target="content\Scripts" />
    <file src="dist\js\boosted*.js.map" target="content\Scripts" />

    <file src="scss\**\*.scss" target="contentFiles\Content\bootstrap" />
    <file src="dist\js\boosted*.js" target="contentFiles\Scripts" />
    <file src="dist\js\boosted*.js.map" target="contentFiles\Scripts" />
  </files>
</package>
